{
  "name": "cdn",
  "version": "1.3.0",
  "description": "A CDN for my personal use.",
  "main": "./prod/index.js",
  "engines": {
    "node": "^15.10.0",
    "npm": "^7.6.0"
  },
  "scripts": {
    "prebuild": "rm -rf ./prod",
    "build": "tsc",
    "postbuild": "cp -r ./src/assets ./prod/assets && cp -r ./src/pages ./prod/pages",
    "lint": "eslint src",
    "start": "node -r dotenv/config prod/index.js",
    "test": "echo 'No tests yet'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/cdn.git"
  },
  "keywords": [
    "cdn",
    "typescript",
    "express"
  ],
  "author": "Nicholas Carrigan",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/nhcarrigan/cdn/issues"
  },
  "homepage": "https://github.com/nhcarrigan/cdn#readme",
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/express-rate-limit": "5.1.1",
    "@types/node": "14.14.40",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@sentry/integrations": "^6.2.3",
    "@sentry/node": "^6.2.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "winston": "^3.3.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
